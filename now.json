{
    "version": 2,
    "name": "hello-hubot",
    "builds": [
        {
            "src": "app.js",
            "use": "@now/node-server"
        },
        {
            "src":"assets/**/*",
            "use":"@now/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}
